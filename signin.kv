

<AuthorizationPage>
     id: main_win
     orientation: 'vertical'
     spacing: 10
     padding: .1, .1
     space_x: self.size[0]/3
     canvas.before:
          Color:
               rgba: (1,1,1,1)
          Rectangle:
               size: self.size
               pos:  self.pos
     Image:
          source: 'Images/Logo.png'
          size_hint_x: .9
          size_hint_y: .9
          pos_hint: {'center_x': .5, 'center_y': .3}
     BoxLayout:
          size_hint_y: None
          height: 50
          canvas.before:
               Color:
                    rgba: (0,.8,.8,1)
               Rectangle:
                    size: self.size
                    pos:  self.pos
          Label:
               text: "Авторизация пользователя"
               bold: True
               size_hint_x: .1
               font_size: 40

     BoxLayout:
          orientation: 'vertical'
          padding: main_win.space_x, 10
          spacing: 20
          BoxLayout:
               orientation: 'vertical'
               spacing: 10
               size_hint_y: .30
               size_hint_x: 1
               height: 100
               resize: True
               Label:
                    id: info
                    text: " "
                    markup: True
                    size_hint_y: .9
                    height: 10
               TextInput:
                    id: username_field
                    hint_text: "Логин" 
                    multiline: False
                    focus: True
                    on_text_validate: pwd_field.focus= True
               TextInput:
                    id: pwd_field
                    hint_text: "Пароль"
                    multiline: False
                    password: True
                    on_text_validate: root.validate_user()
          Label:
               id: sp
               size_hint_y: None
               height: 10
          Button:
               text: "Войти"
               size_hint_y: .2
               size_hint_x: .9
               font_size: 25
               height: 20
               background_normal: 'Images/Button/BtOff1.png'
               on_release: 
                    root.validate_user()
                    root.manager.transition.direction = 'left'
          Label:
               id: sp2


<MainPage>
     name: 'main_page'
     orientation: 'vertical'
     spacing: 10
     canvas.before:
          Color:
               rgba: (1,1,1,1)
          Rectangle:
               size: self.size
               pos:  self.pos
     
     Image:
          source: 'Images/Logo.png'
          size_hint_x: .9
          size_hint_y: .9
          pos_hint: {'center_x': .5, 'center_y': .3}

     BoxLayout:
          size_hint_y: None
          height: 50
          canvas.before:
               Color:
                    rgba: (0,.8,.8,1)
               Rectangle:
                    size: self.size
                    pos:  self.pos
          Label:
               text: "Выбери действие"
               bold: True
               size_hint_x: .1
               font_size: 40
               
     StackLayout:
          padding: 20
          spacing: 20
          Button:
               text: 'Гнёзда'
               font_size: 25
               height: 70
               size_hint_x: .2
               size_hint_y: .15
               orientation: 'tb-rl'
               background_normal: 'Images/Button/BtOn.png'
               on_release: 
                    root.manager.current = 'mold_sockets'
                    root.manager.transition.direction = 'left'
               
          Button:
               text: 'Заявки'
               font_size: 25
               height: 70
               size_hint_x: .2
               size_hint_y: .15
               orientation: 'tb-rl'
               background_normal: 'Images/Button/BtOn.png'
               on_release: 
                    root.manager.current = 'application'
                    root.manager.transition.direction = 'left'           
                    
          
<MoldSockets>
     name: 'mold_sockets'
     orientation: 'vertical'
     spacing: 10
     canvas.before:
          Color:
               rgba: (1,1,1,1)
          Rectangle:
               size: self.size
               pos:  self.pos
     
     Image:
          source: 'Images/Logo.png'
          size_hint_x: .9
          size_hint_y: .9
          pos_hint: {'center_x': .5, 'center_y': .3}

     Button:
          text: 'Назад'
          height: 50
          font_size: 25
          size_hint: .2, .1
          pos_hint: {'center_x': .875, 'center_y': .125}
          background_normal: 'Images/Button/BtOn.png'
          on_release: 
               root.manager.current = 'main_page'
               root.manager.transition.direction = 'right' 

     BoxLayout:
          size_hint_y: None
          height: 50
          canvas.before:
               Color:
                    rgba: (0,.8,.8,1)
               Rectangle:
                    size: self.size
                    pos:  self.pos
          Label:
               text: "Выбери комплекс"
               bold: True
               size_hint_x: .1
               font_size: 40
               pos_hint: {'center_x': .5,}


     GridLayout:
          cols: 3
          padding: 15
          spacing: 15
          row_default_height: 100
          row_force_default: True
          pos_hint: {'center_x': .5, 'center_y': .3}

          Button:
               text: 'Telerobot 1'
               font_size: 25
               background_normal: 'Images/Button/BtOn.png'
               on_release: 
                    root.manager.current: 'socketsT1'
                    root.manager.transition.direction = 'right' 
               
          Button:
               text: 'Telerobot 2'
               font_size: 25
               background_normal: 'Images/Button/BtOn.png'
          
          Button:
               text: 'Telerobot 3'
               font_size: 25
               background_normal: 'Images/Button/BtOn.png'
          
          Button:
               text: 'Telerobot 4'
               font_size: 25
               background_normal: 'Images/Button/BtOn.png'
          
          Button:
               text: 'Telerobot 5'
               font_size: 25
               background_normal: 'Images/Button/BtOn.png'
          
          Button:
               text: 'Telerobot 6'
               font_size: 25
               background_normal: 'Images/Button/BtOn.png'

          Button:
               text: 'Telerobot 7'
               font_size: 25
               background_normal: 'Images/Button/BtOn.png'               


<Application>
     name: 'application'
     orientation: 'vertical'
     spacing: 10
     canvas:
          Color:
               rgba: (1, 1, 1, 1)
          Rectangle:
               size: self.size
               pos:  self.pos
     BoxLayout:
          size_hint_y: None
          height: 50
          canvas.before:
               Color:
                    rgba: (0,.8,.8,1)
               Rectangle:
                    size: self.size
                    pos:  self.pos
          Label:
               id: application_label
               text: ' '
               bold: True
               size_hint_x: .1
               font_size: 40
          Button:
               text: 'Назад'
               font_size: 25
               height: 70
               size_hint_x: .2
               size_hint_y: .15
               pos_hint: {'center_x': .9, 'center_y': .1}
               background_normal: 'Images/Button/BtOff1.png'
               on_release:
                    root.manager.transition.direction = 'right'
                    root.manager.current = 'main_page'

     

